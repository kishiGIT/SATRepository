        <fullName>ABC__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(StageName, &apos;Prospecting&apos;) , Amount * Probability,
IF(ISPICKVAL(StageName, &apos;Qualification&apos;) , Amount * Probability,
IF(ISPICKVAL(StageName, &apos;Proposal Preparation&apos;) , Amount * Probability,
IF(ISPICKVAL(StageName, &apos;Proposal on Desk&apos;) , Amount * Probability,
IF(ISPICKVAL(StageName, &apos;Negotation / Shortlist&apos;) , Amount * Probability,
IF(ISPICKVAL(StageName, &apos;Verbal&apos;) , Amount * Probability,
IF(ISPICKVAL(StageName, &apos;Signed&apos;) , Amount * 1,
0
)))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
