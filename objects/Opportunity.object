        <fullName>Project_ID</fullName>
        <active>true</active>
        <description>Project Code should be in the format - &quot;ABCDEF-ABCD-0000-000&quot;</description>
        <errorConditionFormula>NOT( OR( ISBLANK(Project_Code__c), REGEX( Project_Code__c , &quot;[A-Z]{6}-[A-Z]{4}-[0-9]{4}-[0-9]{3}&quot;)))</errorConditionFormula>
        <errorMessage>Project Code should be in the format - &quot;ABCDEF-ABCD-0000-000&quot;
Please set Caps Lock on</errorMessage>
    </validationRules>
    <validationRules>
